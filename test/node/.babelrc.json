{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6",
          "browsers": [
            "Chrome >= 51",
            "Firefox >= 54",
            "Safari >= 10",
            "Edge >= 15"
          ]
        },
        "modules": "auto",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ]
}